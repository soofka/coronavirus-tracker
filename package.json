    "@lhci/cli": "^0.3.12",
    "babel-jest": "^25.2.6",
    "babel-loader": "^8.0.6",
    "bundlesize": "^0.18.0",
    "closure-webpack-plugin": "^2.3.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.0",
    "express": "^4.17.1",

  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run test:unit:coverage && npm run build:prod && npm run test:e2e",
    "build:dev": "webpack --config webpack.config.base.js",
    "build:prod": "npm run clean && webpack --config webpack.config.prod.js",
    "start": "npm run clean && npm run build:dev && node server.js",
    "test": "npm run test:unit",
    "test:unit": "jest --config jest.config.js",
    "test:unit:coverage": "npm run test:unit -- --coverage | coveralls",
    "test:e2e": "bundlesize",
    "release": "standard-version"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
